{
  "name": "preco",
  "version": "0.0.0",
  "private": true,
  "description": "preco's new website",
  "license": "MIT",
  "author": "",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "repository": "",
  "scripts": {
    "// Run these two to get the dev server running.": "It will begin running on: http://localhost:4200",
    "ajax": "(cd ajax && nodemon entry.js)",
    "dev": "ember serve --proxy http://127.0.0.1:8080 --output-path dist-dev/",
    "// Run build to get the production ready environment.": "Run serve to serve it",
    "build": "yarn run build:prod",
    "build:prod": "ember build --environment production",
    "build:dev": "ember build --environment development",
    "serve": "node fastboot-server.js",
    "// Run deploy to install deps and run fastboot app.": "Install first, then restart the server",
    "deploy:install": "yarn install --production && (cd dist && yarn install)",
    "deploy:serve": "node fastboot-server.js",
    "// Testing and linting": "These are ember defaults",
    "lint:js": "eslint ./*.js app config lib server tests",
    "test": "ember test"
  },
  "devDependencies": {
    "broccoli-asset-rev": "^2.4.5",
    "ember-ajax": "^3.0.0",
    "ember-cli": "^3.1.3",
    "ember-cli-app-version": "^3.0.0",
    "ember-cli-babel": "^6.7.1",
    "ember-cli-dependency-checker": "^2.0.0",
    "ember-cli-eslint": "^4.2.1",
    "ember-cli-fastboot": "^1.1.3",
    "ember-cli-htmlbars": "^2.0.1",
    "ember-cli-htmlbars-inline-precompile": "^1.0.0",
    "ember-cli-inject-live-reload": "^1.4.1",
    "ember-cli-qunit": "^4.1.1",
    "ember-cli-sass": "^7.1.7",
    "ember-cli-shims": "^1.2.0",
    "ember-cli-sri": "^2.1.0",
    "ember-cli-uglify": "^2.0.0",
    "ember-data": "^3.1.1",
    "ember-export-application-global": "^2.0.0",
    "ember-fetch": "^3.4.5",
    "ember-load-initializers": "^1.0.0",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-resolver": "^4.0.0",
    "ember-source": "^3.1.1",
    "ember-welcome-page": "^3.0.0",
    "eslint-plugin-ember": "^5.0.0",
    "loader.js": "^4.2.3"
  },
  "engines": {
    "node": "^4.5 || 6.* || >= 7.*"
  },
  "dependencies": {
    "dotenv": "^5.0.1",
    "express": "^4.16.3",
    "fastboot-app-server": "^1.1.0",
    "nodemon": "^1.17.3"
  }
}
